<div class="{{tag classes}}">
    <p class="entry-title"><a href="{{tag slug}}">{{tag title}}</a></p>
    <div class="entry-image {{tag mobile}}"><a href="{{tag slug}}" title="{{tag title}}"><img src="{{tag thumb}}" alt="{{tag title}}"/></a></div>
    {{tag mobile_images}}
    <p class="entry-date">{{tag date}}</p>
</div>
