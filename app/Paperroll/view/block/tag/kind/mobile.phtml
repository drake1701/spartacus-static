<div class="col-xs-6 col-sm-4 col-md-3 entry-item">
    <p class="entry-title">
        <a href="{{url}}">{{title}}</a>
    </p>
    <div class="entry-image">
        <a href="{{url}}" title="{{title}}">
            <img class="lazy" data-original="{{mobile_big_thumb}}" alt="{{title}}">
        </a>
    </div>
    <p class="entry-date">{{publishedAt_short}}</p>
</div>